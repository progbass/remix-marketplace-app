{
  "name": "remix-marketplace-app",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./tailwind/tailwind.css -o app/styles/app.css",
    "dev": "concurrently --raw \"npm run dev:node\" \"npm run dev:css\"",
    "dev:node": "remix dev -c \"node --inspect server.mjs\"",
    "dev:css": "tailwindcss -w -o ./app/styles/app.css",
    "format": "prettier --write .",
    "lint": "eslint --ignore-path .gitignore --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "typecheck": "tsc -b",
    "cypress:open": "yarn run cypress open",
    "cypress:run": "yarn run cypress run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@algolia/autocomplete-js": "^1.17.0",
    "@algolia/autocomplete-plugin-query-suggestions": "^1.17.0",
    "@algolia/autocomplete-plugin-recent-searches": "^1.17.0",
    "@algolia/autocomplete-theme-classic": "^1.17.0",
    "@algolia/recommend-react": "^1.14.0",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@remix-run/css-bundle": "^2.8.0",
    "@remix-run/express": "^2.4.0",
    "@remix-run/node": "^2.4.0",
    "@remix-run/react": "^2.4.0",
    "@remix-run/serve": "^2.4.0",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "algoliasearch": "^4.22.1",
    "date-fns": "^3.6.0",
    "express": "^4.18.2",
    "i18n-js": "^4.4.3",
    "isbot": "^3.7.1",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone-esm": "^15.0.1",
