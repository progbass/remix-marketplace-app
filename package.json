    "concurrently": "^8.2.2",
    "cypress": "^13.6.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "instantsearch.css": "^8.1.0",
    "prettier": "^3.1.1",
